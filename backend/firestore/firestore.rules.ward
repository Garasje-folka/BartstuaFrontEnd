rules_version = '2';

match /users/{userDoc}/dropInReservations/{reservationDoc} {
  allow read: userDoc == request.auth.uid
}

match /users/{userDoc}/bookingReservations/{reservationDoc} {
  allow read: userDoc == request.auth.uid
}

match /events/{locationDoc}/dropInEvents/{eventDoc} {
  allow read: true
}

match /events/{locationDoc}/bookingEvents/{eventDoc} {
  allow read: true
}
